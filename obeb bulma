
#include <stdio.h>
int obeb(int num1,int num2);

int main() {
    int num1,num2;;
    printf("sayÄ± giriniz: ");
    scanf("%d",&num1);
    scanf("%d",&num2);
    int result=obeb(num1,num2);
    printf("\n%d",result);
    return 0;
}
int obeb(int num1, int num2){
    if(num2 !=0){
    printf("\nnum1 %d num2 %d",num1,num2);
        return obeb(num2,num1%num2);
        }
    else{
        printf("\nnum1 %d num2 %d",num1,num2);
    
        return num1;
    }
    
}
